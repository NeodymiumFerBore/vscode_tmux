{
    "name": "tmux",
    "displayName": "tmux",
    "description": "tmux language support",
    "version": "0.2.0",
    "publisher": "malmaud",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tmux",
                "aliases": [
                    "tmux"
                ],
                "extensions": [
                    ".tmux.conf",
                    ".tmux"
                ],
                "configuration": "./language-configuration.json",
                "filenames": [
                    "tmux.conf"
                ]
            }
        ],
        "grammars": [
            {
                "language": "tmux",
                "scopeName": "source.tmux",
                "path": "./syntaxes/tmux.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/malmaud/vscode_tmux"
    }
}
